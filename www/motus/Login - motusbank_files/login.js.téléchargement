"use strict";$(function(){var e=$("[data-username-field]"),c=$("[data-memorized-account-hidden]"),o=$("select[name='Memorized']"),t=$("input[type='password']"),i=$("input.remember-me-checkbox"),a=$("[data-remember-shown]"),n=$("#remove-account"),r="True"===$("#FirstLogin").val();a.hide(),o.length?(c.hide(),o.focus()):e.focus(),r||o.val("other").selectmenu("refresh"),o.change(function(){var o=$(this).val();if("other"===o)return n.hide(),c.show(),r?e.val("").focus():t.focus(),void a.toggle(i.is(":checked"));c.hide(),n.show(),a.hide(),i.prop("checked",!1),e.val(o),t.focus()}).change(),i.change(function(){var e=$(this).is(":checked");a.toggle(e)}),n.click(function(){$.ajax({url:"Security_ForgetMemorizedAccount",method:"POST",data:{identifier:o.val()},success:function(){location.reload()}})}),$("select[name='AccountType']").change(function(){var e=$(this).val();window.location.replace(e)})});