n      }\n    });\n\n    // handle events and delegate\n    chatbot.eventHandler = async function( data ){\n      let { eventName, eventData, callback } = data;\n\n      switch ( eventName ){\n        case \'dialogue\':\n          var event = new CustomEvent("dialogue.handler",\n            {\n                detail: {\n                  callback: callback,\n                  eventData: eventData,\n                },\n                bubbles: true,\n                cancelable: true\n            }\n          );\n          window.dispatchEvent(event);\n          break;\n\n        case \'conversationStart\':\n          window.dispatchEvent(new CustomEvent("open.handler"));\n          break;\n\n        case \'conversationEnd\':\n          window.dispatchEvent(new CustomEvent("close.handler"));\n          break;\n\n        case \'chatbotUIMinimized\':\n          window.dispatchEvent(new CustomEvent("minimize.handler"));\n          break;\n\n        case \'weblinkClicked\':\n          var event = new CustomEvent("weblinkClick.handler",{detail: eventData});\n          window.dispatchEvent(event);\n          break;\n\n        case \'buttonsRendered\':\n          console.log(\'*** In buttonsRendered\');\n          let buttonsEscalation = [];\n\n          for ( let button of eventData.buttons ){\n            // Each button object will contain the agreed up metadata properties\n            // These properties come from Wysdom\'s backend, so they can be updated any time\n            let { escalation, CHAT_ESCALATION_OPTION_KEY, CHAT_ESCALATION_OPTION_CODE, buttonElement } = button;\n\n            // collect the escalation buttons for reporting purposes\n            //The NO_THANKS is captured as one of the options presented but clicking NO_THANKS is not considered as an esccalation\n            if ( escalation ){\n              buttonsEscalation.push(CHAT_ESCALATION_OPTION_CODE);\n              tangerineButtonEvents(buttonElement, CHAT_ESCALATION_OPTION_CODE);\n            }\n            else if(button.internalLink) {\n              let event = new CustomEvent("internalLinkDisplayed.handler",{detail: button.eventName});\n              window.dispatchEvent(event);\n\n            }\n            // bind your logic to the button\n            tangerineButtonHandler( button );\n          }\n          // do something with the collected escalation buttons (buttonsEscalation)\n          if (buttonsEscalation.length !== 0) {\n           window.dispatchEvent(new CustomEvent("escalationOptions.handler", {detail: buttonsEscalation}));\n          }\n          break;\n\n        default:\n          break;\n      }\n    }\n\n    function tangerineButtonEvents(buttonElement, CHAT_ESCALATION_OPTION_CODE){\n      if ( buttonElement && CHAT_ESCALATION_OPTION_CODE ){\n        buttonElement.addEventListener(\'click\', function(ev){\n          window.dispatchEvent(new CustomEvent("escalation.handler", {detail: CHAT_ESCALATION_OPTION_CODE}));\n        });\n      }\n    }\n\n    function tangerineButtonHandler( button ){\n      let { buttonId, eventName, buttonElement, internalLink, url } = button;\n\n      // the default behaviour for clicking a button or weblink will still happen; to stop it, inform the Wysdom team and they will add a special property to the button on the backend\n      if ( buttonElement && eventName ){\n        buttonElement.addEventListener(\'click\', function(ev){\n          let buttonText = buttonElement.getAttribute(\'data-button-label\');\n          var event = new CustomEvent("click.handler",\n            {\n                detail: {\n                  eventName: eventName,\n                  buttonText: buttonText,\n                  internalLink: internalLink,\n                  url: url\n                }\n            }\n          );\n          window.dispatchEvent(event);\n        });\n      }\n    }\n\n    document.addEventListener(\'proactive\', function(event){\n      proactiveScenarioHandler( event.detail.scenarioName, event.detail.query );\n    });\n\n    function proactiveScenarioHandler( proactiveScenarioName, query ){\n      // check if chatbot is installed\n      if ( chatbot && chatbot.isInstalled() ){\n        // check if the chatbot is not busy with anything else\n        let isChatbotIdle = ( chatbot.getChatState() == \'init\' );\n\n          // for each scenario, send the corresponding special message and data\n          switch( proactiveScenarioName ){\n            // Pin cases\n            case \'pinRetry\':\n              if ( isChatbotIdle ){\n                chatbot.addData( { pin:\'RETRY\' }, \'merge\' );\n                chatbot.sendMessageObj( { query: query, showUserMessage:\'N\' } );\n                chatbot.openChat();\n              }\n              break;\n            case \'pinLockedOut\':\n              chatbot.addData( { pin:\'LOCKED_OUT\' }, \'merge\' );\n              chatbot.sendMessageObj( { query: proactiveScenarioName, showUserMessage:\'N\' } );\n              chatbot.openChat();\n              break;\n\n            // Secret cases\n            case \'secretRetry\':\n              if ( isChatbotIdle ){\n                chatbot.addData( { secretQuestion:\'RETRY\' }, \'merge\' );\n                chatbot.sendMessageObj( { query: query, showUserMessage:\'N\' } );\n                chatbot.openChat();\n              }\n              break;\n            case \'secretLockedOutResetEnabled\':\n              chatbot.addData( { secretQuestion:\'LOCKED_OUT\', resetEnabled:true }, \'merge\' );\n              chatbot.sendMessageObj( { query: proactiveScenarioName, showUserMessage:\'N\' } );\n              chatbot.openChat();\n              break;\n            case \'secretLockedOutResetDisabled\':\n              chatbot.addData( { secretQuestion:\'LOCKED_OUT\', resetEnabled:false }, \'merge\' );\n              chatbot.sendMessageObj( { query: proactiveScenarioName, showUserMessage:\'N\' } );\n              chatbot.openChat();\n              break;\n            case \'proactivePinReset\':\n              chatbot.closeChat();\n              break;\n            case \'minimizeChatbot\':\n              chatbot.toggleChatbox()\n              break;\n\n            default:\n              break;\n          }\n      }\n    }\n    ',this.deregistrationHooks=[],this.fabVisible=!1,this.hideFabWhenProactiveMinimize=!1}return e.prototype.ngOnInit=function(){var e=this;this.environmentService.isNative()||(this.chatbotEnabled=this.environmentService.isEnabled(_.vh.CHATBOT_ENABLED),this.logger.debug("*** chatbotEnabled :",this.chatbotEnabled),this.sessionService.initSession().then((function(){e.chatbotEnabled&&e.sessionService.isAuthenticated()&&(e.logger.debug("*** Chatbot - Session Service init session"),e.fabVisible=!0,e.initChatbot())})),this.legacyLoginService.getSessionCreated.then((function(){e.chatbotEnabled&&(e.logger.debug("*** Chatbot - Login Service session created"),e.initChatbot(),e.wysdomChatbotService.setSessionCreated(!0))})))},e.prototype.loadChatbotCss=function(){var e=document.createElement("link");e.setAttribute("rel","stylesheet"),e.setAttribute("href","assets/css/chatbot.css"),document.head.appendChild(e)},e.prototype.loadChatbotScript=function(){var e=this,t=document.body,n=document.createElement("script");n.src="assets/js/chatbot.js",n.defer=!0,n.onload=function(){var n=document.createElement("script");n.type="text/javascript",n.textContent=e.chatbotConfig,t.appendChild(n)},t.appendChild(n)},e.prototype.initChatbot=function(){var e=this,t={urlVars:{orangeKey:this.cookies.get(_.vh.ORANGE_KEY)}},n=this.wysdomChatbotService.getUserToken();this.wysdomChatbotService.updatePrimaryAccount(n||""),this.isPageWhiteListed=this.wysdomChatbotService.isPageWhiteListed(),this.logger.debug("*** isPageWhiteListed",this.isPageWhiteListed),this.requesterService.request(o.AVAILABLE_SERVICES.GET_CHATBOT_AVAILABLE,t).then((function(t){e.logger.debug("*** response.chatbotAvailable",null==t?void 0:t.chatbotAvailable),(null==t?void 0:t.chatbotAvaila